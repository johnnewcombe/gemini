/*======================================================================*/
STATIC VOID                            /*				*/
end_it_all()                           /* Dump the final buffer to tape */
/*======================================================================*/

{

STATIC
WORD	status,
	retval;

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* If the block is empty (ie last block was exactly full) do nothing	*/
if (buf.buf_used != 0) {

/* If short blocks are not allowed pad the block with the padding	*/
/* character								*/
    if (swtch.bk_short == FALSE) {
	while (buf.buf_used++ < swtch.bk_size)
					    *buf.buf_ptr++ = swtch.bk_pad;
	buf.buf_free = 0;
	--buf.buf_used;
    }

/* Write the block to tape						*/
    dump_buff();
}

/* Write 3 tape marks and reposition before the first of them		*/
wfm(3,&retval);
status = space(-1,'B',&retval);
while ((status & 0x02) != 0)  status = space(-1,'B',&retval);
space(1,'B',&retval);

}

