get_sh(swtch)                                 /* Get short block status */
struct	_switch	*swtch;			/* Structure of switch values	*/
/*======================================================================*/

{

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

swtch->bk_short = -1;			/* Prime to a silly value	*/

/* Is definition in the switches					*/
if ((indx = fndswt(n_swtch,swnames,"SH")) != FAIL) swtch->bk_short = TRUE;

/* .. no so is it a default						*/
else if (deflt == TRUE)  swtch->bk_short = FALSE;

/* .. no so get it from the operator					*/
if (swtch->bk_short == -1) {
    tpchat(0,4,5,3,0);					/* ... allowed?	*/

/* Default is no							*/
    if ((c = getkey()) == ESC)  return(FAIL);
    swtch->bk_short = (BOOLEAN) (toupper(c) == 'Y');
}

return(SUCCESS);

}

