{

LOCAL
UWORD	trcount;		/* Number of bytes to transfer		*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* While bytes left to send						*/
while (size != 0) {

/* While bytes left to send & room left in buffer			*/ 
/* Number of bytes to send = min of dsize & bfleft			*/
    trcount = (size < buf.buf_free) ? size : buf.buf_free;

/* Copy into buffer							*/
    cpymem(ptr,buf.buf_ptr,trcount);

/* Update counters							*/
    ptr += trcount;
    buf.buf_ptr += trcount;
    buf.buf_used += trcount;
    buf.buf_free -= trcount;
    size -= trcount;

/* If not enough room in the buffer dump the buffer to disc		*/
    if (buf.buf_free == 0)  if (dump_buff() == FAIL)  return(FAIL);
}

return(SUCCESS);

}

