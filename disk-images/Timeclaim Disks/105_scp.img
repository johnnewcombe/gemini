get_pb(swtch)                                  /* Get padding character */
struct	_switch	*swtch;			/* Structure of switch values	*/
/*======================================================================*/

{

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

defined = FALSE;

/* Only bother if short blocks are NOT allowed				*/
if (swtch->bk_short == TRUE) {
    swtch->bk_pad = DEF_PAD;		/* Any old value will do	*/
    return(SUCCESS);
}

/* Is it defined in the switch list					*/
if ((indx = fndswt(n_swtch,swnames,"PB")) != FAIL) {
    if (*(swvalues[indx]) != '\0') {
	swtch->bk_pad = corhex(swvalues[indx]);
	defined = TRUE;
    }
}

/* Not defined in switch list so do we take a default			*/
else if (deflt == TRUE) {
    swtch->bk_pad = DEF_PAD;		/* Yes set to default		*/
    defined = TRUE;
}

/* Not defined anywhere so ask operator					*/
if (defined == FALSE) {
    tpchat(0,6,7,3,2);
    if (lgetlin(gstring,3) != 0)  return(FAIL);
    swtch->bk_pad = (gstring[0] == 0) ? DEF_PAD : corhex(gstring);
    defined = TRUE;
}

return(SUCCESS);

}

