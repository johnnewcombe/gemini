STATIC STATUS                                         /*		*/
get_bs(swtch)                                         /* Get block size */
struct	_switch	*swtch;			/* Structure of switch values	*/
/*======================================================================*/

{

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

defined = FALSE;
if ((indx = fndswt(n_swtch,swnames,"BS")) != FAIL) {
    if (*(swvalues[indx]) != NULL) {
	swtch->bk_size = gtkbnm(swvalues[indx]);
	if (swtch->bk_size >= MINBKSZ && swtch->bk_size <= MAXBKSZ)
    							   defined = TRUE;
	else  tperr(0);			/* Block size out of range	*/
    }
}
else if (deflt == TRUE) {
    swtch->bk_size = DEFBKSZ;
    defined = TRUE;
}

/* While blocksize undefined or invalid ask operator for size		*/
while (defined == FALSE) {

/* Get size of maximum block number ready for answer box		*/
/* Enter blocksize							*/
    tpchat(0,1,2,3,c = numlen(MAXBKSZ));	/* Max size		*/

/* Enter a number							*/
    if (lgetlin(gstring,++c) != 0)  return(FAIL);

/* Take default on RET only						*/
    if (gstring[0] == 0)  swtch->bk_size = DEFBKSZ;

/* Translate number that might contain a 'K' char in it			*/
    else swtch->bk_size = gtkbnm(gstring);
    if (swtch->bk_size >= MINBKSZ && swtch->bk_size <= MAXBKSZ)
							   defined = TRUE;
    else  tperr(0);			/* Block size out of range	*/
}

return(SUCCESS);

}

