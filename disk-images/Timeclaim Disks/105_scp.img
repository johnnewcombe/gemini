/*======================================================================*/
STATIC STATUS                                /*				*/
dump_buff()                                  /* Dump the buffer to tape */
/*======================================================================*/

{


STATIC
STATUS	rv;

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Dump the buffer to tape						*/
rv = (mtwrite(buf.buf_ptr - buffer,buffer) != 0) ? FAIL : SUCCESS;

/* Reset the Buffer Control Structure					*/
buf.buf_ptr = buffer;
buf.buf_free = swtch.bk_size;
buf.buf_used = 0;

}

