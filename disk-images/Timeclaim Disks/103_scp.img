/*======================================================================*/
STATIC STATUS                       /*					*/
dptable()                           /* Compute the disc parameter table */
/*======================================================================*/

{

/* All we have to do is set the vector to the allocation area		*/

LOCAL
WORD	wsize;		/* Size of workspace required for current format*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

dph[dn].dph_csv = NULLPTR;	/* No check for changed disc		*/

/* Workspace is required only for CPM					*/
/* M Drive should be detected somewhere and treated separately		*/
wsize = (systyp == CPM) ? (dpbf.dpb_dsm + 1) / 8 + 1 : 0;
if (dn == floppy[SOURCE]) {
    if (dtbl->dct_biosend + wsize >= dph[dn + 1].dph_alv)  return(FAIL);
    dph[dn].dph_alv = dtbl->dct_biosend;/* This is OK if the test fails	*/
}
else {
    if (MEMTOP - wsize <= dtbl->dct_biosend +
			 (dpb[dn - 1].dpb_dsm + 1) / 8 + 1)  return(FAIL);
    dph[dn].dph_alv = MEMTOP - wsize;	/* This is OK if the test fails	*/
}

return(SUCCESS);

}

