STATIC STATUS                                          /*		*/
rd_data(lev)                                           /* Read a record */
struct	lvl	*lev;		/* Pointer to an AMIE Level structure	*/
/*======================================================================*/

{

LOCAL
struct	pkthdr	*pkt;		/* Pointer to a packet header		*/

LOCAL
WORD	nbytes;			/* Number of bytes read from tape	*/

#if	BARCLAYS						/*!!!!!!*/
STATIC				/* To remember its value between calls	*/
LONG	nrecs = 0,		/* Count of number of records copied	*/

	nent;			/* Number of entries in Trailer		*/

LOCAL
WORD	i;			/* Loop counter				*/
#endif								/*!!!!!!*/

#if	LLOYDS							/*!!!!!!*/
LOCAL
BYTE	*ptr,			/* Working pointer			*/
	buffer[R_MAX];		/* Space for maximum size of record	*/

STATIC
WORD	tp_read();		/* Read next record from tape		*/
#endif								/*!!!!!!*/

LOCAL
BYTE	id[32];			/* ASCII copy of Customer ID		*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

#if	BARCLAYS						/*!!!!!!*/
/* Get a packet from the pool						*/
pkt = qget(lev->in_q);

/* Read a record from the tape						*/
/* NEVER done when bc.bk_data == 0					*/
if ((nbytes = tp_read(pkt->d_start,106)) == FAIL) {
    lev->err_action = DISCONN;
    return(tperr(15));
}

/* Test if the record is not a header or trailer			*/
if (*pkt->d_start != 'C') {

/* Count the record							*/
    ++nrecs;

/* Set up packet header							*/
    pkt->d_bytes = rs;
    pkt->d_freesp -= rs;
}

/* Test if the record is a Trailer record				*/
else if (strncmp(pkt->d_start,"CUSTOMER TRAILER",16) == MATCH) {

/* Check that it is for the correct customer				*/
    if (strncmp(cust_id,pkt->d_start + 17,6) != MATCH) {
	lev->err_action = DISCONN;
	seterr("BTCDP");
	return(nferr(2));		/* Header / Trailer mismatch	*/
    }

/* Check that the correct number of records have been transferred	*/
    nent = 0L;
    for (i = 0; i < 7; ++i)
	     nent = nent * 10L + (LONG) (*(pkt->d_start + 23 + i) & 0x0f);
    if (nrecs != nent) {
	lev->err_action = DISCONN;
	seterr("BTCDP");
	return(nferr(3));		/* Incorrect number of records	*/
    }

/* Read the next record							*/
/* NEVER done when bc.bk_data == 0					*/
    nbytes = tp_read(pkt->d_start,106);

/* Check that it is a Header record					*/
    if (strncmp(pkt->d_start,"CUSTOMER HEADER ",16) == MATCH) {
	
/* Set up the customer number and the output record size		*/
	for (i = 0; i < 6; ++i)  cust_id[i] = *(pkt->d_start + 17 + i);
	cust_id[6] = NULL;
