/*======================================================================*/
STATIC STATUS			/*					*/
fget1s(sfd,tstring,maxlen)   	/* get CRLF terminated string from file */
FILE	sfd;			/* file	descriptor of source file	*/
char	*tstring;		/* start of target string		*/
int	maxlen;			/* max no of chars to transfer		*/
/*======================================================================*/

{

/* modified from TLIB FGETS.C to read complete string not stopping on 	*/
/* control characters							*/


LOCAL
WORD	c,			/* holds chars from getc		*/
	strcnt;			/* character counter			*/

WORD	spgetc();

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/
strcnt = 0;				/* zero character count		*/

while ((BYTE)(c = spgetc(sfd)) != '\n' && c != -1) 
			      if(strcnt++ < maxlen)  *tstring++ = (BYTE)c;

if (c == -1)  return(FAIL);		/* end of file			*/
else {
    *tstring = '\0';			/* null terminator		*/
    return(SUCCESS);
}

}

