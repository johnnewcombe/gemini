if ((txtfd = spopen(fname,"RCD")) == FAIL)  return(FAIL);
	
/* Allocate space for text						*/
if ((txtbse = ptr = spalloc((UWORD) filesize(txtfd))) == (BYTE *) FAIL)
								  CLEANUP;

/* Read in text								*/
while (max-- != 0) {

/* Read a string							*/
    if (getstr(txtfd,ptr,80) == FAIL) {
	*array++ = 0;
	while (max-- != 0)  *array++ = 0;
	break;
    }
/* Point to start of string						*/
    *array++ = ptr;

/* Move pointer to start of next string					*/
    while (*ptr++ != NULL) ;
}

cleanup:

spclose(txtfd);		/* close text file				*/

/* Return text descriptor						*/
return(txtbse);

}

