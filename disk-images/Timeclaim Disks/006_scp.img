	if (i == L_FIRST || i == L_MID) {	/* Dummy assignment	*/
	    line[0] = '\'';
	    for (i = 1; i < 7; ++i)  line[i] = spgetc(in_fd);
	    line[i] = NULL;
	    strcat(line,"\' I - - CPM[dv:b] -");
	}
