.he                                                            LLOYDS(AUXOUT)
.pa
*/
#include	"portab.h"

/*======================================================================*/
WORD                           /*					*/
auxout(c)                      /* Send a byte to the Auxilliary Printer */
WORD	c;						/* Byte to send	*/
/*======================================================================*/
/*USE
Portability	Level 4 (Assembler)
Parent Module	Lloyds Bank ECS Special Software
Revision History
10/04/95 IOBYTE used to enable output of labels to serial device - ICFO
23/02/89 Created from TCLAB - ICFO
17/01/89 PIOOUT is a cut down version of the EMMA function with the
         console checking removed - PN
*/

{

/* If IOBYTE PTP: is set to UP1: then we direct the label output to the	*/
/* aux printer whose init and direct output is done in this prog.	*/
/* If IOBYTE PTP: is set to SER: then we direct the label output to the */
/* serial device using the BDOS Punch output                            */
/* Otherwise we direct the label output to the standard parallel printer*/

STATIC
VOID	pioout();		/* Send a character to the PIO		*/

STATIC
WORD    n;                      /* Value of IOBYTE                      */

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Test the value of IOBYTE						*/
if (((n = bdos(7,0) & 0x30)) == 0x20)  pioout(c); /* Aux parallel printr*/
else if (n == 0x10)                    bdos(4,c); /* Serial printer     */
else                                   bdos(5,c); /* Standard printer   */

return(c);
}

/*======================================================================*/
/*
.he                                                   LLOYDS(AUXOUT / PIOOUT)
.pa
*/

#asm
pport::	defb	14h	; PIO base for GM816 for aux par printer support
#endasm

/*======================================================================*/
STATIC VOID                      /*					*/
pioout(c)                        /* Send character to PIO as Centronics */
int	c;				/* Character to be output	*/
/*======================================================================*/

{

/* Cut down version of emma driver (prisend). Console checking removed.	*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

#asm
	.z80
	ld	hl,2		; Load argument to (l)
	add	hl,sp
	call	h.##		; hl = (hl)
p1:	ld	a,(pport)
	ld	c,a		; handshake port address
	in	a,(c)		; Ready status line
	rrca			; Busy? zero bit into carry
	jr	c,p1		; Yes - wait
	inc	c		; Point to data port
	out	(c),l		; Send the character
	dec	c		; Point back to control port
	ld	h,0fdh		; Strobe byte
p2:	in	a,(c)		; Wait until free (ie character accepted)
	rrca
	jr	c,p2
	out	(c),h		; Strobe the character out - strobe on
	ld	h,0ffh		; set the bit to one
	out	(c),h		; strobe off
	xor	a
	ld	h,a		; Return character in (hl)
				; return supplied by C80
	.8080
#endasm

}

