/* Get the switch values and set up 'lev'				*/
/* Allocate a buffer underneath the overlay to stop fragmentation	*/
/* Use 'txt_009' to save a variable					*/
/* Take care that AMIE.009 is less than 256 bytes or fragmentation will	*/
/* occur								*/
	if ((txt_009 = alloc(256)) == FAIL)  return(nferr(1));

#if	LLOYDS							/*!!!!!!*/
	if ((ovl = ldprl("LTCDP.OV1")) == FAIL)
					     return(flerr("LTCDP.OV1",4));
#else								/*!!!!!!*/
#if	BARCLAYS						/*!!!!!!*/
	if ((ovl = ldprl("BTCDP.OV1")) == FAIL)
					     return(flerr("BTCDP.OV1",4));
#endif						/* BARCLAYS	!!!!!!!!*/
#endif						/* LLOYDS	!!!!!!!!*/

	free(txt_009);

	txt_009 = (*ovl)(&control,lev,table,cust_id,&first,amie_009,
							&rs,&fs,&fl,msgs);

	spfree(ovl);

	if (rs == FAIL)  return(FAIL);

/* Initialise driver for reading					*/
	i = uhinit(&control,lev,'R',tpmes);

#if	LLOYDS							/*!!!!!!*/
	lev->expf = 2;			/* Set to zero in UHINIT	*/
#else								/*!!!!!!*/
	lev->expf = 1;			/* Set to zero in UHINIT	*/
#endif						/* LLOYDS	!!!!!!!!*/

	return(i);

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

    case RELEASE:

/* Write a new AMIE.009 and reboot the system.  If it ever returns we	*/
/* failed in some way							*/
	trunov('0',&control,msgs,amie_009,cust_id,first,lev);

/* If 'trunov' returns it must have failed so run on into the DISCONN	*/
/* processing								*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

    case DISCONN:
    case ABORT:

	unlink("M:$$$.TMP");

	autorun = NO;

/* Write dummy amie.009 file and reboot					*/
/* This looks inefficient but it gets over 600 bytes of 'wrt009' out	*/
/* of the base of TCDP							*/
	trunov('2',&control,msgs,amie_009,cust_id,rs,lev);

	cls_txt(text);			/* Release text area		*/
	spfree(txt_009);
	cls_txt(t_text);


#if	LLOYDS							/*!!!!!!*/
	free(paddr);
#endif								/*!!!!!!*/

	return(runovl('B',&control,lev));	/* Close tape		*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

    case SELECT_FILE:

/* Tell operator to remove disc from destination drive			*/
#if	BARCLAYS
	if (autorun == YES && first_file == FALSE) {
#else
	if (autorun == YES && first_file == FALSE
				    && type != L_FIRST && type != L_MID) {
#endif
	    if (isdisc(amie_009[3]) == TRUE) {
		dframe[0] = msgs[0];	/* Tape Controlled Disc Prod	*/
		dframe[1] = msgs[1];	/* Remove & label Disc		*/
		dframe[2] = msgs[3];	/* Press a key to continue	*/
		dialogue(0,0,3,dframe,0);
		kflush();
		if ((c = q_beep(alarm[1])) == (WORD) FAIL)  c = getkey();
		if (c == ESC)  bc.bk_data = 0;		/* Stop the run	*/
		else           chat(19,20,NULL,NULL);
	    }
	}

	runovl('7',&control,lev);	
	lev->err_action = (bc.bk_data == 0) ? DISCONN : RELEASE;
	return((first_file == TRUE) ? SUCCESS : FAIL);

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

    case READ_ATTR:

	return(trunov('3',&control,msgs,amie_009,cust_id,rs,lev));

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

    case OPEN_FILE:

#if	LLOYDS							/*!!!!!!*/
/* Allocate space for the Customer's Address				*/
	if ((paddr = alloc(LP_LAB * (CP_LAB + 2))) == FAIL)
							 return(nferr(1));


/* Call the overlay							*/
	cust_id[6] = NULL;		/* Just in case ...		*/
	strcat(f_name,cust_id);

	if (trunov('4',&control,paddr,&type,cust_id,first,lev)
						!= SUCCESS)  return(FAIL);
	
/* Release all but the required amount of memory			*/
	realloc(paddr,strlen(paddr) + 1);

/* Open the temporary file to accumulate this customer's accounts	*/
	switch (type) {

	    case L_ONLY:		/* Only one account		*/
		break;

	    case L_FIRST:		/* First of several accounts	*/

/* Open a new file for this customer					*/
		if ((fd_t = spopen(f_name,"WC")) == OPENFAIL)
						  return(flerr(f_name,3));
		break;

	    case L_MID:	/* Not first or last of several accounts	*/

/* If a file already exists (as it should) open it and skip to the end.	*/
/* However, if the run has been started part way through (using the	*/
/* facility in the Utilities overlay) the file may not exist so create	*/
/* a new one as for L_FIRST.						*/
		if (fexist(f_name) == FALSE) {
		    if ((fd_t = spopen(f_name,"WC")) == OPENFAIL)
						  return(flerr(f_name,3));
		}
		else {

/* Open the existing file for this customer and skip to the end.  This	*/
/* leaves the file ready to receive the data for the next account	*/
		    if ((fd_t = spuopen(f_name,"WC")) == OPENFAIL)
						  return(flerr(f_name,3));
		    spuseek(fd_t,-128L,2);
		    posn = sputell(fd_t);
		    while (spugetc(fd_t) != 0x1a)  ++posn;
		    spuseek(fd_t,posn,0);
		}
		break;

	    case L_LAST:		/* Last of several accounts	*/

/* Open the existing file for this customer for reading.  If it doesn't	*/
