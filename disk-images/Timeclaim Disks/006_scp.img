    do auxout(*ptr1); while (*ptr1++);
    ptr += (strlen(ptr) + 2);		/* 2 NUL at end of string	*/

    strcpy(msg,ptr);			/* LINE 3			*/
    strcat(ptr1 = msg,"\n");
    do auxout(*ptr1); while (*ptr1++);
    ptr += (strlen(ptr) + 2);		/* 2 NUL at end of string	*/

    strcpy(msg,ptr);			/* LINE 4			*/
    ptr1 = msg + strlen(msg);
    while ((*ptr1 = *paddr++) != LF) {	/* Copy to line feed		*/
	if (*ptr1 != CR)  ++ptr1;	/* Ignore carriage return	*/
    }
    *ptr1 = '\0';			/* Add a NULL to make a string	*/
    strcat(ptr1 = msg,"\n");
