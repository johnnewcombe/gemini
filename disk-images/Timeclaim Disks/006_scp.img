/*======================================================================*/
/*
.he                                                APACSLOG(PRTLIN / PRT_HED)
.pa
*/
/*======================================================================*/
VOID                                         /*				*/
prt_hed(horf,txtp1,title)                    /* Output header or footer */
WORD	horf;				/* Header of Footer (0 or 1)	*/
BYTE	**txtp1;			/* Strings to output		*/
struct	_title *title;			/* Printer Control File		*/
/*======================================================================*/

{

/* %D gives Date as "30 Feb 99" (ie 9 characters)			*/
/* %P gives Page Number as "12345" (ie 5 characters)			*/

LOCAL
BYTE	**line,				/* Pointer to strings to putput	*/
	*s;				/* Local copy of the pointer	*/

LOCAL
WORD	nlines,				/* Number of lines to output	*/
	mth,				/* Month number			*/
	nsp,				/* Number of characters O/P by	*/
					/* 'putlju'			*/
	sl;				/* Length of current line	*/

LOCAL
struct	date *d;			/* Pointer to current date	*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

if (horf == HEADER) {
    prtout(CR);			/* So print head is in correct place	*/
    nlines = atoi(*(txtp1 + 14));
    line = txtp1 + 15;
}
else {
    nlines = atoi(*(txtp1 + atoi(*(txtp1 + 14)) + 15));
    line = txtp1 + atoi(*(txtp1 + 14)) + 16;
}

while (nlines-- > 0) {
    for (sl = strlen(s = *line++); sl > 0; --sl) {
	if (*s == '%') {
	    switch (*(s + 1)) {

		case 'D':			/* Date			*/
		    prtout((d = get_date())->day[0]);
		    prtout(d->day[1]);
		    prtout(' ');
		    mth = ((d->month[0] - '0') & 0xff) * 10 +
					       (d->month[1] - '0') & 0xff;
		    lsputstr(*(txtp1 + mth - 1));
		    prtout(' ');
		    prtout(d->year[0]);
		    prtout(d->year[1]);
		    sl -= 9;
		    s += 9;
		    break;

		case 'P':			/* Page number		*/
		    *s += (nsp = putlju(title->page));
		    nsp = 5 - nsp;
		    while (nsp-- > 0) {
			prtout(' ');
			++s;
		    }
		    sl -= 5;
		    break;

		default:
		    prtout(*s++);
	    }
	}
	else prtout(*s++);
    } 
    lsputstr("\n");
}

/* Let C80 supply the return						*/

}

