tp_read(buf,bytes)                        /* Read tape data into buffer */
BYTE	*buf;				/* Target buffer for data	*/
UWORD	bytes;				/* Number of bytes required	*/
/*======================================================================*/

{

/************************************************************************/
/*									*/
/* This  is a special version for Lloyds Bank.   It allows for the fact */
/* that  each  account is in a separate ANSI file on  the  tape.   This */
/* function strips off all Header and Trailer Labels and Tape Marks and */
/* presents that calling function with a stream of records.  It returns */
/* -2  (the  terminating value) when a Tape Mark is read instead  of  a */
/* Header Label.   It assumes that each Data Block contains an integral */
/* number of Records							*/
/*									*/
/************************************************************************/

LOCAL
STATUS	rv;		/* Return value					*/

LOCAL
WORD	retval;		/* Secondary return from 'space'		*/

LOCAL
UWORD	taken;		/* Number of bytes read from block		*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

rv = SUCCESS;

/* If there are no more bytes left in the current block get a		*/
/* new block if poss							*/
if (bc.bk_data == 0) {

/* Close the current block						*/
    ++bc.bk_curnum;

/* Open the next block							*/
    rv = bki_open();
}

if (rv == SUCCESS && isdigit(*bc.bk_ptr) == FALSE) {

/* Skip over the Tape Mark						*/
    space(1,'F',&retval);
    rv = FAIL;
}

/* We are now situated after the Tape Mark which terminates the Data	*/
/* Skip over the Trailer Labels						*/
/* Note that we do not count these as files so that TCDP still thinks	*/
/* it is in the same file as it started off in				*/
if (rv == FAIL) {
    space(1,'F',&retval);

/* Test if the next block is a Tape Mark - if so we are at the end so	*/
/* return -2								*/
    if (space(1,'B',&retval) != 0) {
	bc.bk_data = 0;		/* So that SELECT_FILE will DISCONN	*/
	return(-2);
    }

/* Space over the rest of the Header Labels				*/
    space(1,'F',&retval);

/* Read the next Data Block						*/
    bki_open();
}

/* Read the smallest of 'bytes' or 'bk_data'				*/
/* Copy bytes to target buffer						*/
cpymem(bc.bk_ptr,buf,(taken = (bytes < bc.bk_data) ? bytes : bc.bk_data));

/* Update block control variables					*/
bc.bk_free += taken;
bc.bk_data -= taken;
bc.bk_ptr += taken;

return(taken);

}

/*======================================================================*/
/*
.he                                                    TCDP(TCDPINP / BKREAD)
.pa
*/
/*======================================================================*/
STATIC STATUS                 /*					*/
bkread()                      /* Read block from tape into block buffer */
/*======================================================================*/

{

LOCAL
WORD	retval,		/* Secondary return from 'space'		*/
	bkstat,		/* Return value from 'mtread'			*/
	byterr;		/* Number of bytes too long/short		*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Issue the read command						*/
if ((bkstat = mtread(bc.bk_size,bc.bk_base,&byterr)) != 0) {

/* Check out the return value and return the status			*/
    while (tpi_iochk(bkstat,byterr) == FAIL) {

/* If 'tpi_iochk' finds a Tape Mark it spaces back over it.  Goodness	*/
/* nose why.  So we will skip forward over it again			*/
	space(1,'F',&retval);

/* We are now situated after the Tape Mark which terminates the Data	*/
/* Skip over the Trailer Labels						*/
/* Note that we do not count these as files so that TCDP still thinks	*/
/* it is in the same file as it started off in				*/
	space(1,'F',&retval);

/* Test if the next block is a Tape Mark - if so we are at the end so	*/
/* return FAIL								*/
	if (space(1,'B',&retval) != 0) {
	    bc.bk_data = 0;	/* So that SELECT_FILE will DISCONN	*/
	    bc.bk_ptr = bc.bk_base;
	    return(FAIL);
	}

/* Space over the rest of the Header Labels				*/
	space(1,'F',&retval);

/* Read the next Data Block						*/
	if ((bkstat = mtread(bc.bk_size,bc.bk_base,&byterr)) == 0)  break;
    }
}

/* Calculate number  of bytes in the block				*/
/* All chatter about padding bytes removed as they are not relevant to	*/
/* TCDP									*/
bc.bk_data = bc.bk_size - byterr;

/* Number of free bytes = 0						*/
bc.bk_free = bc.bk_size - bc.bk_data;

/* Put block pointer at start of block data				*/
bc.bk_ptr = bc.bk_base;

return(SUCCESS);

}

