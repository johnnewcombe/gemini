    while (++i < n)  do auxout(*ptr); while (*ptr++);

    /* Print the disc label						*/
    ptr = (k_ptr + L1_DATA)->l_ptr;	/* LINE 1			*/
    n = (k_ptr + L1_DATA)->n_lines;
    strcpy(msg,ptr);	
