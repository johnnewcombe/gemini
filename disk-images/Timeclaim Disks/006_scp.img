/*======================================================================*/
STATIC VOID                                  /*				*/
klab_print(paddr,cust_num,k_ptr)             /* Print the Address Label */
BYTE	*paddr,				/* Pointer to Postal Record	*/
	*cust_num;			/* Customer ID string		*/
struct	_kelgray *k_ptr;		/* Pointer to KELGRAY data	*/
/*======================================================================*/

{


LOCAL
FILE	fd;			/* File Descriptor for DataBase		*/

LOCAL
WORD	i,n;			/* Loop counters			*/

LOCAL
BYTE	msg[80],
	b[2],
	*ptr1,
	*ptr;			/* Working pointer			*/

LONG	fnd_cst();		/* Return address of Customer Record	*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

ptr = (k_ptr + L2_PRE)->l_ptr;		/* BEFORE the data for label	*/
n = (k_ptr + L2_PRE)->n_lines;
i = -1;
while (++i < n)  do auxout(*ptr); while (*ptr++);

/* Print the label							*/
ptr = (k_ptr + L2_DATA)->l_ptr;
n = (k_ptr + L2_DATA)->n_lines;
b[1] = '\0';
i = -1;
while (++i < n) {
    strcpy(ptr1 = msg,ptr);
    while ((b[0] = *paddr++) != LF)  if (b[0] != CR)  strcat(msg,b);
    strcat(msg,"\n");
    do auxout(*ptr1); while (*ptr1++);
    ptr += (strlen(ptr) + 2);		/* 2 NUL at end of string	*/
}

ptr = (k_ptr + L2_POST)->l_ptr;		/*  AFTER the data for label	*/
n = (k_ptr + L2_POST)->n_lines;
i = -1;
while (++i < n)  do auxout(*ptr); while (*ptr++);

return;

}

