{

LOCAL
struct	date	start;		/* Start date and time			*/

LOCAL
WORD	c,			/* Character from assignment file	*/
	i;			/* Loop counter				*/

LOCAL
BYTE
#if	LLOYDS							/*!!!!!!*/
	*swnames[1],	/* Pointer to array of pointers to switch names	*/
	*swvals[1],	/* Pointer to array of pointers to switch values*/
