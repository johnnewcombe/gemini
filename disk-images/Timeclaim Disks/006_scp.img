/*
.he                                                APACSLOG(APACSPRC / SIGMA)
.pa
*/
/*======================================================================*/
STATIC WORD                               /*				*/
sigma(money,record,offset,size)           /* Accumulate DEBIT or CREDIT */
struct	_money	*money;		/* Pointer to total so far DEBIT/CREDIT	*/
BYTE	*record;		/* Pointer to the record to be examined	*/
WORD	offset,			/* Offset to money field		*/
	size;			/* Size of money field			*/
/*======================================================================*/

{

/* Note that we cannot hold just pence because the C80 Mathpak will	*/
/*      only hold longs up to 2,147,483,647 which is only just 10	*/
/*      digits and we need 11 digits.					*/

LOCAL
LONG	pds;			/* Number of pounds in this record	*/

LOCAL
WORD	i,			/* Loop counter				*/
	pce;			/* Number of pence in this record	*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Point record at required field					*/
/* Offset has been calculated from the 'position' input through the	*/
/* switches								*/
record += offset;

/* Allow last 2 digits to be pence					*/
size -= 2;

/* Accumulate the pounds						*/
/* Assumes that the numbers are right justified with leading zeros as	*/
/* in fact the specification says.					*/
pds = 0L;
for (i = 0; i < size; ++i)  pds = pds * 10L + (LONG) (*record++ - '0');
money->pounds += pds;

/* Accumulate the pence							*/
pce = 0;
for (i = 0; i < 2; ++i)  pce = pce * 10 + (*record++ - '0');
money->pence += pce;

/* If more than 100 pence correct the pounds				*/
if (money->pence > 99) {
    ++money->pounds;
    money->pence -= 100;
}

if (money->pounds > 999999999) {
    seterr("APACSLOG");
    return(nferr(0));
}
else return(SUCCESS);

}

