#include	"dd.h"
#include	"tcdp.h"

#if	LLOYDS							/*!!!!!!*/
#include	"lloyds.h"
#endif								/*!!!!!!*/

#define		MAXSWT	3	/* AR, AL, + spare			*/

/*======================================================================*/
STATUS                   /*						*/
wrt009(amie_009,cust_id) /* Write an AMIE.009 file for possible restart */
BYTE	**amie_009,	/* Pointer to array of pointers to AMIE.009	*/
	*cust_id;	/* Pointer to Customer ID			*/
/*======================================================================*/
/*USE
Portability	Level 1
Parent Module	TCDP
Revision History:
15/10/89 Only writes AutoRun switch is a real customer number is
         presented and it is found in the TCDP.009 file (for BARCLAYS)
         or the DataBase (for LLOYDS) - ICFO
28/02/89 Lloyds stuff added - ICFO
03/08/88 Special error message handler introduced to allow for ESC to
         abort the run if required - ICFO
30/06/88 If no customer record in database the data is written to
         Drive B - ICFO
09/02/88 Started - ICFO
*/

{

/* Note that to save code space only the final transfers in and out are	*/
/* tested for successful completion on the grounds that if any previous	*/
/* failure occured than the last transfer will also fail		*/

LOCAL
FILE	in_fd,		/* File descriptor for TCDP.009			*/
	out_fd;		/* File descriptor for AMIE.009			*/

LOCAL
STATUS	rv;		/* Return value					*/

LOCAL
WORD	nswtch,		/* Actual number of switches			*/
	c,		/* Character in AMIE Control Switch		*/
	j,		/* Loop counter					*/
	i;		/* Loop counter					*/

LOCAL
BYTE	*namep[MAXSWT],	/* Pointers to switch names			*/
	*valuep[MAXSWT],/* Pointers to switch values			*/
	line0[128],	/* Storage of 1st record from customer file	*/
	line[128];	/* Storage of rest of records			*/

LOCAL
BOOLEAN	found;		/* Set TRUE when Customer ID is found in file	*/

STATIC
STATUS	error();	/* Special error handler			*/

#if	LLOYDS							/*!!!!!!*/
LOCAL
LONG	addr;		/* Disc Address of a Customer Record		*/

LONG	fnd_cst();	/* Return Disc Address of a Customer Record	*/
#endif								/*!!!!!!*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

rv = SUCCESS;			/* Be optimistic			*/
in_fd = out_fd = 0;		/* Both files are closed		*/
found = FALSE;			/* So it can return FAIL if any disaster*/

