#include	"portab.h"
#include	"gtape.h"
#include	"tcdp.h"

#if	LLOYDS							/*!!!!!!*/
#include	"lloyds.h"

EXTERN
BYTE	*cust_id,		/* Account Number			*/
	cust_rec[];		/* Customer ID				*/

EXTERN
WORD	isdisc;			/* TRUE or FALSE if a real live disc	*/
#endif								/*!!!!!!*/

/*======================================================================*/
STATUS                       /*						*/
#if	LLOYDS							/*!!!!!!*/
fill_buf(bc,paddr,type)      /* Fill the buffer from the temporary file */
#else								/*!!!!!!*/
fill_buf(bc)                 /* Fill the buffer from the temporary file */
#endif								/*!!!!!!*/
struct	_bc	*bc;		/* Pointer to a tape control structure	*/
#if	LLOYDS							/*!!!!!!*/
BYTE	*paddr;			/* Pointer to Postal Record storage	*/
WORD	*type;			/* See 'LLOYDS.H' for details		*/
#endif								/*!!!!!!*/
/*======================================================================*/
/*USE
Portability	Level 1
Parent Module	TCDP
Revision History:
27/02/89 Lloyds stuff added - ICFO
09/02/88 Started - ICFO
*/

{

LOCAL
FILE	in_fd;			/* File descriptor for M:$$$.TMP	*/

LOCAL
WORD	nbytes;			/* Number of bytes read from the file	*/

LOCAL
BYTE	c,			/* Character from the file		*/
	*ptr;			/* Pointer into buffer			*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Open the temporary file						*/
if ((in_fd = spopen("M:$$$.TMP","RC")) == OPENFAIL) {
    return(flerr("M:$$$.TMP",3));	/* Unable to open this file	*/
}

#if	LLOYDS							/*!!!!!!*/
/* Read in the Account Number						*/
/* This will overwrite the Account Number set up in TCDPOV1.C		*/
ptr = cust_id;
while ((*ptr++ = spgetc(in_fd)) != NULL)  continue;

/* Read in the 'type', the Customer ID and the Postal Address of the	*/
/* Customer Record							*/
ptr = type;
nbytes = 0;
while (nbytes++ < sizeof(WORD))  *ptr++ = spgetc(in_fd);
nbytes = 0;
ptr = cust_rec;
while (nbytes++ < 6) *ptr++ = spgetc(in_fd);
*ptr = NULL;
isdisc = spgetc(in_fd);			/* TRUE or FALSE		*/
ptr = paddr;
nbytes = 0;
while (nbytes++ < LP_LAB) while ((*ptr++ = spgetc(in_fd)) != LF) continue;
#endif								/*!!!!!!*/

/* Read data until 0x1a seen						*/
nbytes = 0;
ptr = bc->bk_ptr = bc->bk_base;
while ((c = spgetc(in_fd)) != 0x1a) {
    *ptr++ = c;
    ++nbytes;
}

bc->bk_data = nbytes;
bc->bk_free = bc->bk_size - nbytes;

/* Close and erase M:$$$.TMP						*/
spclose(in_fd);

unlink("M:$$$.TMP");

return(SUCCESS);

}

