/*======================================================================*/
STATIC STATUS                          /*				*/
error(file)                            /* Special error message handler */
BYTE	*file;		/* Filename to be included in output if ness	*/
/*======================================================================*/

{

LOCAL
BYTE	emsg[61];	/* Holds error message				*/

LOCAL
WORD	c,		/* Character from operator			*/
	delay;		/* Delay between beeps				*/

LOCAL
FILE	efd;		/* Error file descriptor			*/

LOCAL
BOOLEAN	eferr;		/* Flags an error has happened in error file	*/

EXTERN
WORD	alarm[];	/* As defined in 'AMIE.C'			*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Set up title								*/
/* ***** WARNING *****							*/
dframe[0] = genmes[54];

/* Set up dframe assuming that something will fail in the error file	*/
#if	LLOYDS							/*!!!!!!*/
dframe[1] = "M:LTCDP.ERR";	/* Filename				*/
#else								/*!!!!!!*/
#if	BARCLAYS						/*!!!!!!*/
dframe[1] = "M:BTCDP.ERR";	/* Filename				*/
#endif						/* BARCLAYS	!!!!!!!!*/
#endif						/* LLOYDS	!!!!!!!!*/
dframe[3] = genmes[3];		/* press any key etc			*/

/* Set error file status to FALSE					*/
eferr = FALSE;

/* Open up current error file & get message				*/
if ((efd = spopen(dframe[1],"RCD")) == OPENFAIL) {
    dframe[2] = genmes[39];	/* error file not found			*/
    eferr = TRUE;
}

/* Read the required message into error string				*/
/* Only works for error 0						*/
if (eferr == FALSE) {
    if (getstr(efd,emsg,61) == FAIL) {
	dframe[2] = genmes[40];		/* Read error in error file	*/
	eferr = TRUE;
    }
}

/* If an error occurred then report it and exit				*/
if (eferr == TRUE)  CLEANUP;

dframe[1] = file;
dframe[2] = emsg;
/* dframe 3 is already set to genmes 3					*/

cleanup:

/* Close the error file							*/
spclose(efd);

/* output the error & wait for a keystroke				*/
dialog(7,2,4,dframe,0);
kflush();
if ((c = q_beep(alarm[0])) == (WORD) FAIL)  c = getkey();

