STATIC
BYTE	numsep[] = ",";			/* start of number		*/

LOCAL
FILE	fid;				/* input file id		*/

LOCAL
STATUS	readres;			/* result of read		*/

LOCAL
WORD	strid,				/* string area id		*/
	lpos,				/* working position in line	*/
	ind,				/* conversion index counter	*/
	num,				/* build numbers here		*/
	digit,				/* digit counter		*/
	linenum;			/* input file line counter	*/

LOCAL
BYTE	c,				/* current character		*/
	*strings[FILLEN],		/* Text strings			*/
	line[133];			/* current input line		*/
	
/*!- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

if ((fid = spopen(fil,"RC")) == OPENFAIL)  return(flerr(fil,3));

linenum = ind = 0;

while (ind <= 255 && (readres = fget1s(fid,line,132)) != FAIL) {
    ++linenum;				/* bump line counter		*/
    if (line[0] == ';')  continue;	/* comment			*/
    if ((lpos = index(line,numsep)) == -1)  break;	/* "," missing	*/
    for (digit = 0, num = 0; digit < 3 ; ++digit)
				  if (!isdigit( c = line[++lpos]))  break;
				  else num = num*10 + (WORD)(c-'0');

    if (digit == 0 || num > 255)  break;

/* enter into table 							*/
    else                          table[ind++] = (BYTE) num;
}

spclose(fid);					/* close file		*/

/* handle error here							*/
if (ind != 256 || readres == FAIL) {		/* error occured	*/
    if ((strid = opn_txt(STRFIL,strings,FILLEN)) == FAIL)
						  return(flerr(STRFIL,3));

    strings[4] = fil;				/* set up file name	*/
    strings[6] = itoa(linenum,line);		/* set line number into */
						/*... line as tmp buffer*/
    if (readres == FAIL)  strings[7] = strings[9];/* eof error message	*/
    dialogue(0,0,6,&strings[3],0);		/* display error	*/
    cls_txt(strid);
    getkey();					/* wait for the keypress*/
    return(FAIL);
}
else return(SUCCESS);

}

