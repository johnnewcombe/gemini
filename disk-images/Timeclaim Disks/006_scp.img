/*======================================================================*/
VOID                                   /*				*/
set_rs(s)                              /* Set up the Output Record Size */
BYTE	*s;			/* Pointer to Line 1 of AMIE.009	*/
/*======================================================================*/

{

STATIC
BYTE	khars[3][3] = {
		"036",
		"100",
		"106"
	};

LOCAL
WORD	rec_size,
	char_num,
	i,j;			/* Loop counters			*/

LOCAL
BYTE	*namep[15],		/* Pointers to switch names		*/
	*valuep[15],		/* Pointers to switch values		*/
	*k,			/* Pointer to start of switches in 's'	*/
	buffer[80];		/* Space for copy of switches		*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Skip to the second "'" character assuming that the first is at the	*/
/* beginning of the line						*/
j = 1;
while (*(s + j++) != '\'') ;

/* Skip to the start of the 3rd assignment on the line			*/
for (i = 0; i < 2; ++i) {
    while (*(s + j++) <= ' ') ;
    while (*(s + j++) >  ' ') ;
}
while (*(s + j++) <= ' ') ;

/* Skip to the '['							*/
while (*(s + j++) != '[') ;
k = s + j;

/* Copy the switches to a buffer for parsing				*/
i = 0;
while (*(s + j) != ']' && i < 79)  buffer[i++] = *(s + j++);
buffer[i] = NULL;

/* Parse the switches							*/
i = setswt(10,buffer,namep,valuep);

/* Find the pointer to the value of the 'rs' switch.  The switch MUST	*/
/* be present or an error would have occured in OV1			*/
i = fndswt(i,namep,"RS");

/* Overwrite the value with the record size taken from the Header	*/
/* Record just read							*/
if      (rs ==  36)  rec_size = 0;
else if (rs == 100)  rec_size = 1;
else                 rec_size = 2;		/* Must be 106 then	*/
for (char_num = 0; char_num < 3; ++char_num)
       *(k + (valuep[i] - buffer) + char_num) = khars[rec_size][char_num];

return;

}

