STATIC VOID                                     /*			*/
lab_print(paddr,type)                           /* Print the Disc Label */
BYTE	*paddr;				/* Pointer to Postal Record	*/
WORD	type;				/* See 'LLOYDS.H' for details	*/
/*======================================================================*/

{

LOCAL
FILE	fd;			/* File descriptor for DataBase		*/

LOCAL
WORD	i,j;			/* Loop counters			*/

LOCAL
BYTE	*ptr;			/* Working pointer			*/

LONG	fnd_cst();		/* Returns address of Customer Record	*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

if (type != L_FIRST && type != L_MID && *paddr != NULL && isdisc == TRUE){

/* Test the value of IOBYTE and init if necessary			*/
    if ((bdos(7,0) & 0x30) == 0x20)  initprt();

/* Print the disc label							*/
    auxout(CR);
    auxout(LF);
    for (j = 0; j < OFFSET; ++j)  auxout(' ');
    ptr = "Lloyds Bank Statement Data";
    i = (CP_LAB - strlen(ptr)) / 2;
    while (i-- > 0)  auxout(' ');
    while (*ptr != NULL)  auxout(*ptr++);
    auxout(CR);
    auxout(LF);
    auxout(LF);
    auxout(LF);
    for (j = 0; j < OFFSET; ++j)  auxout(' ');
    ptr = paddr;
    while ((j = *ptr++) != LF)  auxout(j);
    auxout(CR);
    auxout(LF);
    auxout(LF);
    auxout(LF);
    for (j = 0; j < OFFSET; ++j)  auxout(' ');
    ptr = "Date:- ";
    while (*ptr != NULL)  auxout(*ptr++);
    ptr = (BYTE *) get_date();
    auxout(*(ptr + 4));
    auxout(*(ptr + 5));
    auxout('-');
    auxout(*(ptr + 2));
    auxout(*(ptr + 3));
    auxout('-');
    auxout(*ptr);
    auxout(*(ptr + 1));
    i = 0;
    while (i++ < 4)  auxout(' ');
    ptr = "Customer ID:- ";
    while (*ptr != NULL)  auxout(*ptr++);
    ptr = cust_rec;
    while (*ptr != NULL)  auxout(*ptr++);
    auxout(CR);
    auxout(LF);

/* Output the gap to the next label					*/
    for (i = 0; i < LP_GAP; ++i)  auxout(LF);
}

/* Let C/80 supply the return						*/

