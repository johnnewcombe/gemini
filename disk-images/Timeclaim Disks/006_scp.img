STATUS                                       /*				*/
trunov(b,control,msgs,amie_009,cust_id,w,lev)/* Load/run a TCDP overlay */
BYTE	b;				/* Overlay number as character	*/
struct	_ctrl	*control;	/* Control structure			*/
BYTE	**msgs,			/* Messages				*/
	**amie_009,		/* AMIE.009 strings			*/
	*cust_id;		/* Customer ID string			*/
WORD	w;			/* For OV0 = 'first'; for OV3 = 'rs'	*/
struct	lvl	*lev;		/* Pointer to AMIE Level structure	*/
/*======================================================================*/

{

/* This function relies on the generic name being 5 bytes long (ie	*/
/* LTCDP or BTCDP)							*/

#if	LLOYDS							/*!!!!!!*/
STATIC
BYTE	ov_name[] = "LTCDP.OV?";
#else								/*!!!!!!*/
#if 	BARCLAYS						/*!!!!!!*/
STATIC
BYTE	ov_name[] = "BTCDP.OV?";
#endif					/* #if BARCLAYS		!!!!!!!!*/
#endif					/* #if LLOYDS		!!!!!!!!*/

LOCAL
STATUS	(*ov_go)(),
	rv;

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

#if	BARCLAYS | LLOYDS					/*!!!!!!*/
ov_name[8] = b;					/* Select the overlay	*/
#else								/*!!!!!!*/
ov_name[7] = b;					/* Select the overlay	*/
#endif								/*!!!!!!*/

if ((ov_go = ldprl(ov_name)) == FAIL)  return(flerr(ov_name,4));
rv = (*ov_go)(control,msgs,amie_009,cust_id,w,lev);/* Execute it	*/
free(ov_go);					/* Free its memory	*/
return(rv);					/* Return return value	*/

}

