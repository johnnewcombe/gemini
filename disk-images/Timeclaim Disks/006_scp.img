/*======================================================================*/
STATIC STATUS /*							*/
align()       /* Enable the operator to align the labels in the printer */
/*======================================================================*/

{

/* If IOBYTE PTP: is set to UP1: then we direct the label output to the	*/
/* aux printer whose init and direct output is done in this prog.	*/

STATIC
struct	_kelgray *k_ptr;

LOCAL
UWORD	size;			/* For 'allocmax'			*/

LOCAL
WORD	i,j,			/* Loop counters			*/
	n,			/* Number of lines in section		*/
	iobyte;                 /* Setting of IOBYTE			*/

LOCAL
BYTE	msg[71],
	*ptr,
	*ptr1;

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

/* Test the value of IOBYTE and init if necessary			*/
if ((iobyte = bdos(7,0) & 0x30) == 0x20) initprt();

/* 'kelgry' allocates space for 'k_ptr'					*/
if (iobyte == 0x10 && kelgry(&k_ptr) == FAIL)  return;

/* Line up labels in the printer press ESC to abandon CR to go		*/
dframe[0] = *(menu_mess + 16);	/* Align labels in printer		*/
dframe[1] = *(menu_mess + 17);	/* Line up labels in printer		*/
dframe[2] = *(menu_mess + 18);	/* Press ESC to exit, CR to print	*/
dialogue(NULL,NULL,3,dframe,0);
kflush();
while ((i = getkey()) != ESC && i != CR)  continue;

while (i != ESC) {

/* Ask the operator to wait						*/
    chat(19,20,NULL,NULL);

/* Print a label							*/
    if (iobyte == 0x10) {
	ptr = (k_ptr + L_CONNECT)->l_ptr;
	n = (k_ptr + L_CONNECT)->n_lines;
	i = -1;
	while (++i < n)  do auxout(*ptr); while (*ptr++);
	ptr = (k_ptr + L1_PRE)->l_ptr;
	n = (k_ptr + L1_PRE)->n_lines;
	i = -1;
	while (++i < n)  do auxout(*ptr); while (*ptr++);
	ptr = (k_ptr + L1_DATA)->l_ptr;
	n = (k_ptr + L1_DATA)->n_lines;
	i = -1;
	while (++i < n) {
	    strcpy(msg,ptr);
	    j = -1;
	    while (++j < CP_LAB)  strcat(msg,"W");
	    strcat(ptr1 = msg,"\n");
	    do auxout(*ptr1); while (*ptr1++);
	    ptr += (strlen(ptr) + 2);	/* 2 NUL at end of string	*/
	}
	ptr = (k_ptr + L1_POST)->l_ptr;
	n = (k_ptr + L1_POST)->n_lines;
	i = -1;
	while (++i < n) do auxout(*ptr); while (*ptr++);
    }
    else {
/* Ensure carriage is at the left hand side before starting		*/
	auxout(CR);
	for (i = 0; i < LP_LAB; ++i) {			/* Once per line*/
	    for (j = 0; j < OFFSET; ++j)  auxout(' ');	/* Left margin	*/
	    for (j = 0; j < CP_LAB; ++j)  auxout('W');	/* Line of chars*/
	    auxout(CR);
	    auxout(LF);
	}
	for (i = 0; i < LP_GAP; ++i) {
	    auxout(CR);
	    auxout(LF);
	}
    }

/* If OK press ESC to exit or CR for another go				*/
    dframe[0] = *(menu_mess + 16);	/* Align labels in printer	*/
    dframe[1] = *(menu_mess + 17);	/* Line up labels in printer	*/
    dframe[2] = *(menu_mess + 18);	/* Press ESC to exit,CV to print*/
    dialogue(NULL,NULL,3,dframe,0);
    kflush();
    while ((i = getkey()) != ESC && i != CR)  continue;

}
if (iobyte == 0x10)  spfree(k_ptr);
}

