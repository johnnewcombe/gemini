STATIC STATUS                         /*				*/
is_type(s,off,size,record)            /* Is record of a particular type */
BYTE	*s;			/* String containing field values	*/
WORD	off,			/* Offset to field in record		*/
	size;			/* Size of field in record		*/
BYTE	*record;		/* Pointer to the record to be examined	*/
/*======================================================================*/

{

/* Returns SUCCESS if the particular field of the record matches any of	*/
/* the identifying values.						*/

LOCAL
WORD	i;		/* Loop counter					*/

/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/

i = 0;
while (TRUE) {
    if (strncmp(s + i,record + off,size) == MATCH)  return(SUCCESS);
    if (*(s + i + size) != ',')  break;
    i += (size + 1);
}
return(FAIL);

}

