  if symvec.status=1 then
  begin
    sccol(col);
    moveto(round(ssx*xscal),round(ssy*yscal));
    lto(round(scx*xscal),round(scy*yscal));
  end;
  if symvec.status=2 then
  begin
   moveto(round(ssx*xscal),round(ssy*yscal));
   sccol(col);
