procedure put_cursor;
begin
if areanum>0 then
 begin
  sstle(4);
  sccol(red);
  cx:=cx+cx1;
  if cx<5 then cx:=5;
  if cx>762 then cx:=762;
  cy:=cy+cy1;
  if cy<4 then cy:=4;
  if cy>263 then cy:=263;
  moveto(cx-5,cy);
  lto(cx+5,cy);
  moveto(cx,cy-4);
  lto(cx,cy-1);
  moveto(cx,cy+1);
  lto(cx,cy+4);
  moveto(cx,cy);
  sstle(0);         {standard mode}
 end;
end;



