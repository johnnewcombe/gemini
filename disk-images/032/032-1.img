          gotoxy(1,dn+k);clreol;
        end;
        gotoxy(5,dn);
      end;

     end;
     x:=bdos($12);             {Find next entry ......}
     if x<>255 THEN
      printit;
     count:=count+1;
   until x=255;                {.... until no more found}
 end
 ELSE
 begin
   gotoxy(1,10);
   write('No files of extension ',ext,' found on drive ',drv,'.');
 end;
END;



