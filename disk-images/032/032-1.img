begin
 seek(paramf,areanum);
 read(paramf,param);
 with param do
 begin
   if partition=2 then
   begin
    tx:=tx+rightone;
    bx:=bx+rightone;
   end;
        top:=ty+1;
     bottom:=by-1;
       left:=tx+1;
      right:=bx-1;
      sctop:=round(ty*yscal)+1;
