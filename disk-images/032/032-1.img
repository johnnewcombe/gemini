procedure set_area_boundries;
begin
 seek(paramf,areanum);
 read(paramf,param);
 with param do
 begin
      top:=ty+1;
   bottom:=by-1;
     left:=tx+1;
    right:=bx-1;
    curwp:=partition;
    curdp:=partition;
 end;
 setcwp(curwp); setcdp(curdp);
end; {of set_area_boundries}


Procedure draw_vector(sx2,sy2,cx2,cy2,col : integer);
var
  ssx,ssy,scx,scy : integer;
  sysclx, syscly : real;
begin
  set_area_boundries;
  sysclx:=(right-left+1)/symbol_sizex;
  syscly:=(bottom-top+1)/symbol_sizey;
  ssx:=round(sx2*sysclx)+xoffs;
  ssy:=round(sy2*syscly)+yoffs;
  scx:=round(cx2*sysclx)+xoffs;
  scy:=round(cy2*syscly)+yoffs;
  if symvec.status=1 then
  begin
    sccol(col);
    moveto(ssx,ssy);
    lto(scx,scy);
  end;
  if symvec.status=2 then
  begin
   moveto(ssx,ssy);
   sccol(col);
   ffill;
  end;
end;  {of draw_vector}


