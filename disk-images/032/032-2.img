var
   direction : integer;
   sp : integer;

procedure redline(sx,sy,ex,ey : integer);
begin
    moveto(sx,sy);
    sccol(red);
    sstle(0);
    lto(ex,ey);
end;
procedure greenline(sx,sy,ex,ey : integer);
begin
    moveto(sx,sy);
    sccol(green);
    sstle(0);
    lto(ex,ey);
end;


begin
