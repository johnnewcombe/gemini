{Overlay }Procedure Printronix;
begin
  tl:=last_line;
  FOR line:=1 to last_line do
  begin
    gotoxy(54,8);
    write(100-(tl-line)/tl*100:4:1,'% done');
    lastdata:=0;
    FOR crl:=1 to end_one do
    begin
     if eof(f1) then eight[crl]:=0 else
     read(f1,eight[crl]);
     if eight[crl]>0 then lastdata:=crl;
