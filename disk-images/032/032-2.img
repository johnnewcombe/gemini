procedure nothing;
begin
 writeln('');
end;

procedure printit;
begin
    entry:=(x*32)+1;                   {uses 32 bytes. The first 12  }
    ln:=copy(dma,entry+1,8);           {give the drive, name & extension.}
    ln:=copy(ln,1,pos(' ',ln)-1);
    write('*',ln,'*');
    new(newlabel);
    newlabel^.name:=ln;
    if firstlabel=nil then
     firstlabel:=newlabel
    else
     lastlabel^.next:=newlabel;
    lastlabel:=newlabel;
