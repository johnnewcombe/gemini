       ee:=e;
       ff:=f;
       gg:=g;
       hh:=h;
       jj:='';
       kk:=0;
       mm:=m;
     end;
     write(snf,sn);
    end; {while not eof}
    close(sof); close(snf);
 loop:
    if keypressed then firstlabel:=nil else
    firstlabel:=firstlabel^.next;
  end;
  release(heaptop);
