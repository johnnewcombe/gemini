procedure place(b : byte);
begin
  sixline[sixpos]:=b;
  if (sixpos=128) or (finish=TRUE) then
  begin
    blockwrite(fname,sixline,1);
    sixpos:=1;
    fillchar(sixline,sizeof(sixline),0);
  end
  else
  sixpos:=sixpos+1;
end; {of place}


begin    {dump_pluto}
